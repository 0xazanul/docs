  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Overview",
            "pages": ["index", "quickstart", "development"]
          },
          {
            "group": "Product",
            "pages": [
              "product/auth",
              "product/dashboard",
              "product/assets",
              {
                "group": "Settings",
                "pages": [
                  "product/settings/overview",
                  "product/settings/domains",
                  "product/settings/notifications",
                  "product/settings/webhooks"
                ]
              }
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/asset-discovery",
              "guides/remediation",
              "guides/verification"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Introduction",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/endpoint/create",
              "api-reference/endpoint/get",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ]
  },
  "api": {
    "baseUrl": "https://api.qaslylabs.xyz/v1",
    "auth": {
      "method": "bearer",
      "name": "Authorization",
      "inputPrefix": "Bearer "
    },
    "playground": {
      "mode": "show"
