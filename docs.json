{
  "name": "QaslyLabs",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "/"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#0EA5E9",
    "light": "#22D3EE",
